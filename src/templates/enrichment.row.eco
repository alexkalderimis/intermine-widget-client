<td class="check"><input type="checkbox" <%= if @row["selected"]: %>checked="checked"<% end %> /></td>
<td class="description"><%= @row["description"] %></td>
<td class="pValue"><%= @row["p-value"] %></td>
<td class="matches">
    <a class="count" style="cursor:pointer"><%= @row["matches"].length %></a>
</td>